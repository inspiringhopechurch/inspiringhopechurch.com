{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "command": "npm start",
      "name": "Run npm start",
      "request": "launch",
      "type": "node-terminal"
    },
    {
      "command": "npm run build",
      "name": "Run npm build",
      "request": "launch",
      "type": "node-terminal"
    },
    {
      "type": "firefox",
      "request": "launch",
      "reAttach": true,
      "name": "Launch Firefox",
      "url": "http://localhost:8000/",
      "webRoot": "${workspaceFolder}",
      "pathMappings": [
        {
          "url": "file:///Users/orville/Sites/inspiringhope.gatsby/gatsby-browser.js",
          "path": "${workspaceFolder}/gatsby-browser.js"
        },
        {
          "url": "file:///Users/orville/Sites/inspiringhope.gatsby/src",
          "path": "${workspaceFolder}/src"
        }
      ]
    },
    {
      "name": "Launch Chrome",
      "request": "launch",
      "type": "chrome",
      "url": "http://localhost:8000",
      "webRoot": "${workspaceFolder}"
    },
    {
      "type": "firefox",
      "request": "attach",
      "name": "Attach"
    },
  ]
}