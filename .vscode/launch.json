{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "type": "firefox",
      "request": "launch",
      "reAttach": true,
      "name": "Launch Firefox",
      "url": "http://localhost:8000/",
      "webRoot": "${workspaceFolder}",
      "pathMappings": [
        {
          "url": "file:///Users/orville/Sites/inspiringhope.gatsby/gatsby-browser.js",
          "path": "${workspaceFolder}/gatsby-browser.js"
        },
        {
          "url": "file:///Users/orville/Sites/inspiringhope.gatsby/src",
          "path": "${workspaceFolder}/src"
        }
      ]
    },
    {
      "name": "Launch Chrome",
      "request": "launch",
      "type": "pwa-chrome",
      "url": "http://localhost:8000",
      "webRoot": "${workspaceFolder}"
    },
    {
      "type": "firefox",
      "request": "attach",
      "name": "Attach"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Program",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}/develop"
    }
  ]
}
